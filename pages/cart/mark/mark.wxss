/* pages/cart/mark/mark.wxss */
.mark-box {
  position: absolute;
  top: 0;
  bottom: 0;
  width: 100%;
  text-align: center;
}

.mark-pic {
  width: 240rpx;
  height: 144rpx;
  margin: 230rpx auto 0 auto;
}

.mark-pic image {
  display: block;
  width: 100%;
  height: 100%;
}

.mark-title {
  font-size: 32rpx;
  color: #282c39;
  font-weight: 600;
  padding-bottom: 30rpx;
  padding-top: 45rpx;
}

.mark-text {
  font-size: 26rpx;
  color: #a6a6a6;
}