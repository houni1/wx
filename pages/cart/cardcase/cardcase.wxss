/* pages/cart/cardcase/cardcase.wxss */
page{position: absolute;
  top: 0;
  bottom: 0;
  width: 100%;
  height:100%;
  overflow-y:auto;
  }
.cardcase-wrap{width:100%;height:100%;padding:60rpx 30rpx 0 30rpx;box-sizing:border-box;overflow-y:scroll;} 
.caedcase-list{padding:50rpx;margin-bottom:40rpx; box-shadow: 0rpx 4rpx 40rpx 0rpx #C2BEC0;border-radius:30rpx;background-color:#fff;}
.profile image{display:block;float:left;width:98rpx;height:98rpx;border-radius:64rpx;}
.profile-name-wrap{float:left;}
.profile-post-wrap{float:left;}
.profile-name{margin-left:25rpx;margin-top:40rpx;font-weight:bold;}
.profile-post{margin-left:20rpx;margin-top:40rpx;}
.moods-wrap{margin-top:20rpx;margin-left:125rpx;}
.moods-wrap image{display:block;float:left;width:22rpx;height:23rpx;margin-top:9rpx;}
.moods-wrap text{display:block;float:left;}
.moods-in{margin-left:25rpx;margin-right:20rpx;font-size:26rpx;color:#808080;}
.moods-num{font-size:30rpx;color:#f25827;}
.phone-wrap{margin-left:125rpx;margin-top:30rpx;}
.phone-wrap image{display:block;float:left;width:20rpx;height:29rpx;margin-top:5rpx;}
.phone-val{display:block;float:left;margin-left:25rpx;margin-right:20rpx;font-size:26rpx;color:#808080;}
.email-wrap{margin-left:125rpx;margin-top:25rpx;}
.email-wrap image{display:block;float:left;width:24rpx;height:19rpx;margin-top:10rpx;}
.email-val{display:block;float:left;margin-left:25rpx;margin-right:20rpx;font-size:26rpx;color:#808080;}
.company-wrap{margin-top:60rpx;}
.company-name{font-size:32rpx;font-weight:bold;color:#282c39;}
.company{margin-top:45rpx;}
.company image{display:block;float:left;width:27rpx;height:32rpx;margin-top:5rpx;}
.company-warranty{display:block;float:left;margin-left:25rpx;font-size:26rpx;color:#808080;}
.cardcase-state{margin:60rpx auto 0;padding-top:40rpx;border-top:1px dashed #eee;}
.cardcase-state view{width:275rpx;height:85rpx;font-size:30rpx;color:#282c39;line-height:85rpx;text-align:center;border-radius:8rpx;}
.cardcase-refuse{float:left;border:1px solid #e6e6e6;background-color:#fff;}
.cardcase-permit{float:right;border:1px solid #ffcf10;background-color:#ffcf10;}
.cardcase-nothing{position: absolute;left: 50%; top: 35%;transform: translate(-50%, -50%);text-align:center;}
.cardcase-no-data{width:180rpx;height:161rpx;}
.no-cardcase{font-size:32rpx;color:#282c39;}
.change-cardcase{font-size:32rpx;color:#a6a6a6;}
