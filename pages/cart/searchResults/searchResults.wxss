page {
  height: 100%;
  box-sizing: border-box;
  background: #fff;
}
.wrap {
  background:#fff; 
  overflow: hidden; 
  margin-bottom: 138rpx;
}
.flex {
  display: flex;
}
.flex1 {
  flex: 1;
}
.searchInfo {
  padding: 30rpx 30rpx 10rpx;
}
.searchInfo .infoBox {
  padding: 30rpx;
  align-items: center;
  -webkit-box-shadow: #ebebeb 0px 0px 10px;
  -moz-box-shadow: #ebebeb 0px 0px 10px;
  box-shadow: #ebebeb 0px 0px 10px;
  background: #fff;
  border-radius: 10rpx;
}
.searchInfo .photo {
  width: 110rpx;
  height: 110rpx;
  margin-right: 28rpx;
  border-radius: 50%;
  overflow: hidden;
  box-shadow: 5px 5px 5px #ebebeb, -2px 5px 5px #ebebeb; 
}
.searchInfo .name {
  font-size: 26rpx;
  color: #282c39;
  font-weight: bold;
}
.searchInfo .tag {
  font-size: 26rpx;
  color: #282c39;
  padding-top: 20rpx;
}
.searchInfo .photo image{
  width: 110rpx;
  height: 110rpx;
}
.searchInfo .relocate {
  font-size: 26rpx;
  color: #fbc836;
  margin-top: -40rpx;
}
.filterTab {
  width: 100%;
  height: 84rpx;
  font-size: 26rpx;
  color: #28293c;
  align-items: center;
  font-family: 'PingFangSC-Regular';
  background: #fff;
  box-shadow: 0 4rpx 10rpx #f5f5f5;
}
.filterTab .tabItem {
  flex: 1;
  text-align: center;
}
.filterTab .filterActive {
  color: #fcc91a;
  font-weight: bold;
}
.allGoods .filterActive {
  color: #fcc91a;
  font-weight: bold;
}
.filterTab image {
  width: 14rpx;
  vertical-align: middle;
}
.allGoods {
  position: fixed;
  left: 0;
  /* top: 210rpx; */
  top: 0;
  width: 100%;
  height: 100%;
  line-height: 60rpx;
  padding: 20rpx 0;
  z-index: 99;
}
.thiv {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 1000;
  background: rgba(0, 0, 0, 0.3);
}
.allGoods text {
  display: block;
  background: #fff;
  line-height: 90rpx;
  color: #28293c;
  font-size: 28rpx;
  padding-left: 40rpx;
}
.allGoods .active {
  color: #fcc91a;
}
/* 列表 */
.carsList {
  background: #f9f9fa;
}
.item {
  position: relative;
  overflow: hidden;
  margin-bottom: 15rpx;
}
navigator {
  display: flex;
  padding: 28rpx 0;
  background: #fff;
}
.hotTag {
  position: absolute;
  left: 40rpx;
  top: 0;
}
.hotTag image {
  width: 86rpx;
}
.imgWrap {
  width: 330rpx;
  height: 200rpx;
  text-align: center;
}
.item .imgWrap image {
  width: 260rpx; 
  height: 100%;
  vertical-align: middle;
}
.carInfo {
  flex: 1;
  padding-right: 20rpx;
}
.carInfo .title {
  font-size: 28rpx;
  line-height: 42rpx;
  font-weight: bold;
  margin-bottom: 14rpx;
}
.price {
  font-size: 26rpx;
  color: #f72626;
  margin-bottom: 8rpx;
}
.priceNum {
  font-size: 38rpx;
  font-weight: bold;
  display: inline;
}
.guidePrice {
  font-size: 24rpx;
  color: #a6a6a6;
  margin-bottom: 14rpx;
}
.status {
  margin-bottom: 10rpx;
}
.carStatus, .stock {
  display: inline-block;
}
.carStatus {
  padding: 8rpx 20rpx;
  background: #e5f9f6;
  border-radius: 40rpx;
  font-size: 24rpx;
  font-weight: bold;
  color: #01c4a2;
  margin-right: 16rpx;
}
.stock {
  font-size: 24rpx;
  color: #a6a6a6;
}
.shareBtn {
  position: absolute;
  right: 20rpx;
  bottom: 20rpx;
  width: 80rpx;
  height: 80rpx;
}
.shareBtn image {
  width: 40rpx;
}

button {
  width: 80rpx;
  height: 80rpx;
  padding: 0;
  background: #fff;
}
button::after {
  border: 0;
}
.noData {
  width: 100%;
  text-align: center;
}
.noData image{
  width: 380rpx;
  height: 300rpx;
  margin: 270rpx auto 0 auto;
}

.noData .btn-box {
  width: 100%;
  padding: 80rpx;
  box-sizing: border-box;
}

.noData .btn {
  width: 100%;
  height: 100rpx;
  font-size:32rpx;
  color:#282c39;
  line-height:100rpx;
  text-align:center;
  font-weight:bold;
  border-radius:50rpx;
  background-color:#ffcf10;
  box-shadow:5rpx 5rpx 15rpx #ffe68f;
}

.bindCompany {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  padding: 20rpx 30rpx 0;
  height: 138rpx;
  background: #fff;
  box-sizing: border-box;
  box-shadow: 0 -2rpx 2rpx #efefef;
}
.bindCompany .btn {
  width: 100%;
  height: 100rpx;
  font-size:32rpx;
  color:#282c39;
  line-height:100rpx;
  text-align:center;
  font-weight:bold;
  border-radius:50rpx;
  background-color:#ffcf10;
  box-shadow:5rpx 5rpx 15rpx #ffe68f;
}

.mail-mark {
  position: absolute;
  top: 0;
  bottom: 0;
  width: 100%;
  z-index: 10;
  background: rgba(0, 0, 0, 0.5);
}

.mail-box {
  width: 532rpx;
  height: 372rpx;
  background: #fff;
  position: absolute;
  top: 50%;
  left: 50%;
  margin-left: -266rpx;
  margin-top: -320rpx;
  border-radius: 15rpx;
}

.mail-box-title {
  font-size: 32rpx;
  color: #282c39;
  font-weight: 600;
  text-align: center;
  padding-top: 35rpx;
}

.mail-box-input {
  width: 410rpx;
  height: 72rpx;
  background: #eee;
  margin: 60rpx auto 0 auto;
  display: flex;
  flex-direction: column;
  padding-left: 20rpx;
  box-sizing: border-box;
  border-radius: 8rpx;
}

.mail-box-input input {
  flex: 1;
  height: 100%;
  font-size: 26rpx;
  color: #a6a6a6;
}

.mail-box-btns {
  position: absolute;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 90rpx;
  border-top: 1rpx solid #eee;
  display: flex;
  line-height: 90rpx;
}

.mail-box-btns text {
  display: inline-block;
  flex: 1;
  text-align: center;
  font-size: 30rpx;
  font-weight: 600;
}
.cancel-btn {
  color: #282c39;
}
.sure-btn {
  color: #ffcf10;
  border-left: 1rpx solid #eee;
}


.shadow {
  position: fixed;
  z-index: 30;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  background-color: rgba(0, 0, 0, .4)
}
.shadow .contain {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  padding: 40rpx;
  width: 500rpx;
  background-color: #fff;
}
.shadow .contain .ttl {
  margin-bottom: 50rpx;
  font-size: 35rpx;
  color: #2c2c2c;
}
.shadow .contain .desc {
  line-height: 50rpx;
  font-size: 26rpx;
  color: #999;
}
.shadow .btn-wrapper {
  margin-top: 40rpx;
  text-align: right;
  font-size: 28rpx;
  color: #2c2c2c;
}
.shadow .btn-wrapper .confirm {
  position: relative;
  display: inline-block;
  margin-left: 50rpx;
  color: #1AAD16;
}
.shadow .btn-wrapper .confirm button {
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  margin: auto;
  opacity: 0;
}